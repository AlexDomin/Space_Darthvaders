<Window x:Class="Space_Invaders.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Space_Invaders"
        mc:Ignorable="d"
        Title="Space Darthvaders" Height="500" Width="800">
    <Canvas Name="myCanvas" Focusable="True" KeyDown="KeyIsDown" KeyUp="KeyIsUp">
        <Canvas.Background>
            <ImageBrush ImageSource="pack://application:,,,/images/Death Star 3.jpg"/>
        </Canvas.Background>

        <Label Name="enemiesLeft" Foreground="White" FontSize="16" FontWeight="ExtraBold">Vaders Left: 0</Label>

        <Rectangle Name="player" Fill="White" Height="65" Width="55" Canvas.Left="388" Canvas.Top="376"></Rectangle>
        
    </Canvas>
</Window>
